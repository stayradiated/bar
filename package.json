{
  "name": "bar",
  "version": "0.0.1",
  "description": "bar to display text",
  "main": "app/index.html",
  "window": {
    "toolbar": true,
    "height": 100,
    "width": 1000,
    "resizable": false,
    "as_desktop": true,
    "frame": false,
    "transparent": true
  },
  "scripts": {
    "test": "mocha --compilers coffee:coffee-script test",
    "build": "coffee -c -o app/js/ source",
    "watch": "coffee -c -w -o app/js/ source",
    "css": "sass --watch source/styles/main.scss:app/style.css"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stayradiated/bar.git"
  },
  "keywords": [
    "bar"
  ],
  "author": "George Czabania",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/stayradiated/bar/issues"
  },
  "dependencies": {
    "coffee-script": "~1.6.3",
    "coffee-scrunch": "~0.1.4"
  }
}
